{
  "plugins": [
    [
      "module-resolver", {
        "root": ["./src"],
        "alias": {
          "styles": "./src/styles"
        },
        "cwd": "babelrc"
    }],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ],
    "macros"
  ],
  "presets": [
    "next/babel"
  ],
  "ignore": []
}